    "url": "git@github.com:pmorjan/weblogin.git"
  },
  "dependencies": {
    "compression": "^1.6.0",
    "express": "^4.13.3",
    "helmet": "^1.1.0",
    "htpasswd": "^2.2.1",
    "http-auth": "^2.2.8",
    "morgan": "^1.6.1",
    "node-forge": "^0.6.38",
    "pty.js": "^0.3.0",
    "ursa": "^0.9.1",
    "ws": "^1.0.1"
  },
  "bin": {
    "weblogin": "./bin/weblogin.js"
  }
}
