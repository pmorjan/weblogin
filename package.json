{
  "name": "weblogin",
  "version": "0.1.0",
  "description": "server login via web browser",
  "main": "app.js",
  "author": "Peter Morjan <pmorjan@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prepasswd": "touch .htpasswd",
    "passwd": "htpasswd .htpasswd",
